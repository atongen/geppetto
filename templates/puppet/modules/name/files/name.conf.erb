server {
  listen                *:80;

  server_name           _;

  access_log            /var/log/nginx/www.cakes.com.access.log;
  error_log             /var/log/nginx/www.cakes.com.error.log;

  root  /var/www/www.cakes.com/current/public;

  passenger_enabled on;
  passenger_ruby /opt/rubies/ruby-1.9.3-p545/bin/ruby;
}
